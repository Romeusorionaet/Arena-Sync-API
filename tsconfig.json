{
    "compilerOptions": {
      "target": "es2022",
      "module": "commonjs",
      "baseUrl": "./",
      "paths": {
        "@/*": [".src/*"]
      },
      "outDir": "./dist",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
    }
  }